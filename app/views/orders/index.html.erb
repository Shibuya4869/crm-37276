<%= render "shared/header" %>

<h1>orders/index</h1>
<%= link_to "契約情報登録", new_order_path %>

<br><br>
<% if @orders == [] %>
  <p>契約情報が登録されていません</p>
<% else %>
  <% @orders.each do |order| %>
    <%= link_to "編集", edit_order_path(order.id) %>
    <%= order.client_id %>
    <%= order.service_id %>
    <%= order.user_id %>
    <%= order.order_date %>
    <%= order.expiry %>
    <%= order.license %>
    <%= order.note %>
    <br>
  <% end %>
<% end %>
