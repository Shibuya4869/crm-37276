<h2>サービス情報編集</h2>

<%= form_with model: @service, local: true, class:"form-edit" do |f| %>
  <%= f.label :name, "サービス名", class:"form-label" %>
  <%= f.text_field :name %>
  <%= f.label :price, "サービス単価", class:"form-label" %>
  <%= f.text_field :price  %>
  <%= f.submit "更新", class:"form-btn" %>
<% end %>
